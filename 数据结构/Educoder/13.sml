val N = getInt();

fun insert( X , N) = 
	if N > 0 
		then
			let
				val tmp = getInt()
				val Y = tmp :: X
			in
				insert(Y, N - 1)
			end
		else
			X;
fun rotation( [], ys) = ys
  | rotation(x :: xs, ys) = rotation(xs, x::ys);
  
val X = insert([], N);

val Y = rotation(X, []);

val Z = List.drop(Y, N div 2);

printIntTable(Z);

























