val N1 = getInt();
val N2 = getInt();
val T1 = getIntTable(N1);
val T2 = getIntTable(N2);

fun append_reverse ([], update) = update
  | append_reverse (x::xs, update) = append_reverse(xs, x::update);

printIntTable(append_reverse(T1@T2, []));


